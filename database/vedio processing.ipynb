    "import cv2"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "vedio=cv2.VedioCapture('sample.mp4')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "while True:\n",
    "    state,frame=vedio.read()\n",
    "    if not state:\n",
    "        break\n",
    "    cv2.imshow('vedio output',frame)\n",
    "    if cv2.waitkey(1)==27:\n",
    "        break\n",
    "vedio.release()\n",
    "cv2.destroyAllWindows()"
   ]
